{
    "name": "marknotton/browser",
    "description": "Query the server-side information from the users agent data.",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "browser"
    ],
    "support": {
        "docs": "https://github.com/https://github.com/marknotton/craft3-plugin-browser/browser/blob/master/README.md",
        "issues": "https://github.com/https://github.com/marknotton/craft3-plugin-browser/browser/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Notton",
            "homepage": "https://github.com/marknotton/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1",
        "jenssegers/agent": "^2.6"
    },
    "autoload": {
        "psr-4": {
          "marknotton\\browser\\": "src/"
        }
    },
    "extra": {
        "name": "Browser",
        "handle": "browser",
        "schemaVersion": "1.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/https://github.com/marknotton/craft3-plugin-browser/browser/master/CHANGELOG.md",
        "components": {
            "browserService": "marknotton\\browser\\services\\BrowserService"
        },
        "class": "marknotton\\browser\\Browser"
    }
}
